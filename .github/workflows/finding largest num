num1=int(input("Enter a number: "))
num2=int(input("Enter a number: "))
num3=int(input("Enter a number: "))

if num1>num2:
    largest_num=num1
if num1>num3:
    Largest_num=num1
    
if num2>num1:
    largest_num=num2
if num2>num3:
    Largest_num=num2
    
if num3>num1:
    largest_num=num3
if num3>num2:
    Largest_num=num3
    
print("largest number is: ",largest_num)
